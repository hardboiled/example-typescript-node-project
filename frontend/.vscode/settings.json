{
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
    ],
    "[typescript,typescriptreact]": {
      "editor.tabSize": 2,
      "editor.insertSpaces": true
    },
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
    },
  }
